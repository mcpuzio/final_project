<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<p>
	<%= form_for @user do |f| %>
		<%= f.label :name %>
		<%= f.text_field :name %>
		<%= f.label :address %>
		<%= f.text_field :address %>
		<%= f.label :password %>
		<%= f.password_field :password %>
		<%= f.submit %>
	<% end %>
</p>
